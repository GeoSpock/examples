{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "impliedStrict": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "rules": {
        /* Standard overrides */
        "no-unused-vars": ["error", {
            "argsIgnorePattern": "^_|unused",
            "varsIgnorePattern": "^_|unused"
        }]
    },
    "globals": {
        "process": false
    }
}
